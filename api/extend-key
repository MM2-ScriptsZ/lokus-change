// api/extend-key.js
export default function handler(req, res) {
  const { key } = req.body;
  const keyData = extendKeyExpiration(key); // Replace with your DB update logic

  if (keyData) {
    res.status(200).json({ message: "Key extended" });
  } else {
    res.status(400).json({ message: "Failed to extend key" });
  }
}
