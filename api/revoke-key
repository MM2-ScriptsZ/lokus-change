// api/revoke-key.js
export default function handler(req, res) {
  const { key } = req.body;
  const success = revokeKeyFromDatabase(key); // Replace with your DB delete logic

  if (success) {
    res.status(200).json({ message: "Key revoked" });
  } else {
    res.status(400).json({ message: "Failed to revoke key" });
  }
}
